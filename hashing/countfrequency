//using two for loop

![countfreq-twoLoops](https://user-images.githubusercontent.com/70484186/188310695-aa95ef14-090e-483c-9e99-2549624bd844.jpeg)


void countfreq(int arr[],int n){
vector<bool> visited(n,false);
for(int i=0;i<n;i++){
  if(visited[i]==true)
    continue;
    
  int c=1;
  for(int j=i+1;j<n;j++){
    if(arr[i]==arr[j]){
      visited[j]=true;
      c++;
    }
  }
  cout<<arr[i]<<" "<<c;
}
}
